{
  "name": "music-app",
  "version": "1.0.0",
  "description": "Server for serving and handling request for music-app",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "client-build": "cd client && npm run build",
    "client-install": "cd client && npm install",
    "server": "nodemon server.ts",
    "start": "ts-node server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malkmus87/music-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malkmus87/music-app/issues"
  },
  "homepage": "https://github.com/malkmus87/music-app#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.4",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
